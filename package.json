{
  "name": "excel-merch-backend",
  "version": "1.0.0",
  "description": "Excel Merchandise backend",
  "main": "index.js",
  "scripts": {
    "generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "dev": "npm run generate && nodemon src/index.ts",
    "prebuild": "npm run generate",
    "build": "tsc",
    "postbuild": "copyfiles --flat ./src/utils/mailTemplates/*.html ./dist/utils/mailTemplates",
    "migrate:deploy": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "prestart": "npm run generate && npm run migrate:deploy",
    "start": "node dist/index.js",
    "cron": "node dist/cronRunner.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/copyfiles": "^2.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/localtunnel": "^2.0.4",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.8.2",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "localtunnel": "^2.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.8",
    "nodemon": "^3.0.1",
    "razorpay": "^2.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0"
  }
}
